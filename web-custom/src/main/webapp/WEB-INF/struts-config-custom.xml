<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

          
<struts-config>

	<action-mappings>
        <action    path="/parse-accounts"
                   type="br.com.auster.dware.console.vivo.web.ParseUploadedSirsAction"
                   scope="request">
               <forward        name="show-accounts" path="/show-accounts.do"/>
        </action>
		
	</action-mappings>
	
  <controller inputForward="true" tempDir="/tmp"/>

  <message-resources parameter="br.com.auster.dware.console.vivo.messages.CustomMessages" key="custom" />
   
</struts-config>

	          
